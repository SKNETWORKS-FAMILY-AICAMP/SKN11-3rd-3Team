   "source": [
    "import zipfile\n",
    "import os\n",
    "\n",
    "# 압축 파일 경로와 압축 해제할 위치 지정\n",
    "zip_path = \"game_data.zip\"\n",
    "extract_to = \"game_data\"\n",
    "\n",
    "# 폴더가 없으면 생성\n",
    "os.makedirs(extract_to, exist_ok=True)\n",
    "\n",
    "# zip 파일 열고 압축 해제\n",
    "with zipfile.ZipFile(zip_path, 'r') as zip_ref:\n",
    "    zip_ref.extractall(extract_to)\n",
    "\n",
    "print(\"압축 해제 완료!\")\n"
   ]
